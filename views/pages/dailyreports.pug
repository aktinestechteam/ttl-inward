block content
	// ==============================================================
	// Container fluid
	// ==============================================================
	.container-fluid
		// ==============================================================
		// Start Page Content
		// ==============================================================
		.row.mb-3
			.card(style='width: 100%;')
				.card-body
					h4   Daily Challan Reports
					form(enctype='multipart/form-data', action='/generatedailychallanreports', method='POST')
						input(type='hidden', name='_csrf', value=_csrf)
						.row.mb-3
							.col-lg-3
								label Station
								select#inwardcargo_dailychallanreports_city.select2.form-control.custom-select(type='text', name='inwardcargo_dailychallanreports_city', autocomplete="off")
									each airport in airportlistdetails
										option(value=airport.iata_code) #{airport.iata_code} (#{airport.city_name})
							.col-lg-2
								label Payment Mode
								select#inwardcargo_dailychallanreports_paymentmode.select2.form-control.custom-select(type='text', name='inwardcargo_dailychallanreports_paymentmode', autocomplete="off")
									option(value='CASH', selected='') CASH
									option(value='CHQ') CHQ/DD
							.col-lg-2
								label From Date
								.input-group
									input#inwardcargo_dailychallanreports_from_date_input.form-control(type='text', name='inwardcargo_dailychallanreports_from_date_input', autocomplete="off")
							.col-lg-2
								label To Date
								.input-group
									input#inwardcargo_dailychallanreports_to_date_input.form-control(type='text', name='inwardcargo_dailychallanreports_to_date_input', autocomplete="off")
							.col-lg-3
								label &nbsp
								button#inwardcargo_dailychallanreports_get_btn.btn.btn-danger.waves-effect.waves-light.save-category.w-100(type='submit')
									i.fas.fa-file-excel(aria-hidden='true')
									| &nbsp&nbsp Challan Reports
