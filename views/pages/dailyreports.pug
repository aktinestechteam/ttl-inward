										option(value=airport.iata_code) #{airport.iata_code} (#{airport.city_name})
							input#inwardcargo_monthlygstreports_date_input.form-control(type='hidden', name='inwardcargo_monthlygstreports_date_input', autocomplete="off")
							.col-lg-2
								label Select Month
								select.select2.custom-select.form-control#inwardcargo_monthlygstreports_select_month
									- var current_month = new Date().getMonth();
									- var months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
									each month, i in months
										if current_month === i
											option(value=i, selected)=month
										else
											option(value=i)=month
							.col-lg-2
								label Select Year
								select.select2.custom-select.form-control#inwardcargo_monthlygstreports_select_year
									- var current_year = new Date().getFullYear();
									- for(let i = current_year - 2; i <= current_year + 1; i++)
										if current_year === i
											option(value=i, selected)=i
										else
											option(value=i)=i
							.col-lg-2
								label &nbsp
								button#inwardcargo_monthlygstreports_get_btn.btn.btn-danger.waves-effect.waves-light.save-category.w-100(type='submit') 
