block specific-js
	// this page js
	script(src='/extra-libs/multicheck/datatable-checkbox-init.js')
	script(src='/extra-libs/multicheck/jquery.multicheck.js')
	script(src='/js/swal_helper.js')
	script.
		$("#inwardcargo_monthlygstreports_select_month").change(function () {
			setMonthlyReportTimestamp();
		});
		
		$("#inwardcargo_monthlygstreports_select_year").change(function () {
			setMonthlyReportTimestamp();
		});
		
		function setMonthlyReportTimestamp() {
			let month = $("#inwardcargo_monthlygstreports_select_month option:selected").val();
			let year = $("#inwardcargo_monthlygstreports_select_year option:selected").val();
			
			let date = new Date();
			date.setDate(5);
			date.setMonth(month);
			date.setYear(year);
			
			$('#inwardcargo_monthlygstreports_date_input').val(date.getTime());
		}
	
		//	Set the time stamp at the load time so that we have a default value to work with
		setMonthlyReportTimestamp();
		$("#inwardcargo_all_pending_documents_option_date_range_to").datepicker();
		$("#inwardcargo_all_pending_documents_option_date_range_to").datepicker("option", "dateFormat", 'yy-mm-dd' );
		$("#inwardcargo_all_pending_documents_option_date_range_to").datepicker('setDate', new Date());
		$("#inwardcargo_all_pending_documents_option_date_range_from").datepicker();
		$("#inwardcargo_all_pending_documents_option_date_range_from").datepicker("option", "dateFormat", 'yy-mm-dd' );
		$("#inwardcargo_all_pending_documents_option_date_range_from").datepicker('setDate', new Date());
		
		$("input[name=inwardcargo_all_pending_documents_option]:radio").click(function() {
			console.log($(this).attr("value"));
			switch($(this).attr("value")) {
				case 'inwardcargo_all_pending_documents_option_date_range': {
					$("#inwardcargo_all_pending_documents_option_date_range_to").removeAttr('disabled');
					$("#inwardcargo_all_pending_documents_option_date_range_from").removeAttr('disabled');
					$("#inwardcargo_all_pending_documents_document_type").removeAttr('disabled');
					
					$('#inwardcargo_all_pending_documents_awb_number').attr('disabled', '');
