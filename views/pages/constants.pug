			.col-12.mt-3
				.card
					.table-responsive
						table.table
							thead.thead-light
								tr
									th(scope='col')
										b IATA
									th(scope='col')
										b DO
									th(scope='col')
										b HAWB (BB)
									th(scope='col')
										b Baggage
									th(scope='col')
										b DD
									th(scope='col')
										b Misc
									th(scope='col')
										b Effective From
									th(scope='col')
										b Expires on
									th(scope='col')
										b Edit
									
							tbody.customtable
								each cityconstant in cityconstantdetails
									tr
										td #{cityconstant.iata_code}
										td #{cityconstant.delivery_order_charge}
										td #{cityconstant.break_bulk_charges}
										td #{cityconstant.delivery_order_charge_baggage}
										td #{cityconstant.direct_delivery_charges}
										td #{cityconstant.miscellaneous_charges}
										td #{new Date(cityconstant.effective_from).toLocaleString('en-US', date_format_options)}
										if (cityconstant.expires_on < sails.config.globals.expires_at_infinity)
											td #{new Date(cityconstant.expires_on).toLocaleString('en-US', date_format_options)}
										else
											td Active
										td
											if ((cityconstant.effective_from < Date.now()) && (cityconstant.expires_on == sails.config.globals.expires_at_infinity))
												button.btn.btn-danger.waves-effect.waves-light(onclick="showModal('" + cityconstant.id + "','" + escape(cityconstant.bank_ifsc) + "','" + escape(cityconstant.registered_address) + "','" + escape(cityconstant.bank_address) + "','" + escape(cityconstant.delivery_address) + "','" + escape(cityconstant.customs_address) + "','" + cityconstant.gstin_number + "','" + cityconstant.hsn + "'," + cityconstant.igst_percentage + "," + cityconstant.cgst_percentage + "," + cityconstant.sgst_percentage + "," + cityconstant.collect_fees_percentage + "," + cityconstant.collect_fees_min_usd + "," + cityconstant.delivery_order_charge + "," + cityconstant.delivery_order_charge_baggage + "," + cityconstant.direct_delivery_charges + "," + cityconstant.break_bulk_charges + "," + cityconstant.miscellaneous_charges + "," + cityconstant.cartage_charge_min_weight + "," + cityconstant.cartage_charge_min + "," + cityconstant.cartage_charge_per_kg + ",'" + cityconstant.signatory + "','" + cityconstant.designation + "','" + cityconstant.iata_code + "'," + JSON.stringify(cityconstant.can_front) + "," + JSON.stringify(cityconstant.can_back) + ", " + cityconstant.effective_from + ", '" + cityconstant.gst_budget_code + "','" + cityconstant.gst_budget_account + "','" + cityconstant.approved_by + "','" + cityconstant.approver_title + "','" + cityconstant.approver_email + "','" + cityconstant.intimation_email + "','" + cityconstant.line_manager_email + "','" + cityconstant.approval_manager_email + "')", ) 
