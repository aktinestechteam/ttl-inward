	// Bread crumb and right sidebar toggle
	// ==============================================================
	.page-breadcrumb
		.row
			.col-12.d-flex.no-block.align-items-center
				h4.page-title Elements
				.ml-auto.text-right
					nav(aria-label='breadcrumb')
						ol.breadcrumb
							li.breadcrumb-item
								a(href='#') Home
							li.breadcrumb-item.active(aria-current='page') Library
	// ==============================================================
	// End Bread crumb and right sidebar toggle
	// ==============================================================
	// ==============================================================
	// Container fluid
	// ==============================================================
	.container-fluid
		// ==============================================================
		// Start Page Content
		// ==============================================================
		.row
			.col-12
				.card
					.card-body
						h5.card-title.m-b-0 Badges
					table.table
						thead
							tr
								th(scope='col') Badges
								th(scope='col') Description
						tbody
							tr
								td
									span.badge.badge-primary Primary
								td
									code .badge .badge-primary
							tr
								td
									span.badge.badge-secondary Secondary
